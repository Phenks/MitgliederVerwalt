        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/Farben.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Schriftarten.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/Text.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
